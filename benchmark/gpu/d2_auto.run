#SBATCH --mem 40960
 
slmodules -s x86_E5v2_Mellanox_GPU
module load gcc cuda cudnn mvapich2 openblas
source ~/tensorbayes/tensorflow-1.9/bin/activate
srun python TensorBayes_v4.2_gpu_auto.py 50000 100 20 > d2_auto.log
