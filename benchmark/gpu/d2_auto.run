#!/bin/bash -l
#SBATCH --nodes=1
#SBATCH --time=12:0:0
#SBATCH --qos=gpu
#SBATCH --gres=gpu:1
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=1
